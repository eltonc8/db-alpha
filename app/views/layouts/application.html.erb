<!DOCTYPE html>
<html>
<head>
  <title>dbAlpha</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= yield %>

<div class="nav-bar-js"></div>
<div class="container-fluid" id='db-alpha'></div>

<script>
  $(function () {
    window.trigger_true = {trigger: true}
    DbAlpha.Views.navbar = new DbAlpha.Views.Navbar({el: $("div.nav-bar-js")});
    DbAlpha.Routers.router = new DbAlpha.Routers.DbAlphaRouter({$rootEl: $("#db-alpha")});
    Backbone.history.start();
  })
</script>


</body>
</html>
